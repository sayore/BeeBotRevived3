{
  "compilerOptions": {
      "module": "node16", 
      "target": "es2021",
      "moduleResolution": "node16",
      "lib": [
          "es2021", "ESNext",
      ],
      "sourceMap": true,
      "declaration": true,
      "skipLibCheck": true,
      "pretty": true,
      "noEmit": false,
      "checkJs": false,
      "outDir": "./dist",
  },
  "include": ["src/App.ts", "./src/Test.ts" ],
  "exclude": [
      "node_modules",
      "./dist"
  ]
}